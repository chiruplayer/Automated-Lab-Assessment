extends layout

block content
  h1 Question Title
  p Question body
  
  .form(id='myform' name='myform' align='center')
    h3 Code
    textarea(rows='13' cols='100' id='code' name='code')
    br
    h3 Input
    textarea(row='10' cols='100' id='input' name='input')
    br
    | Language
    select(name='lan' id='lan')
      option(value='C') C
    br
    | Compile with input:
    input(type='radio' name='inputRadio' id='inputRadio' value='true') 
    | Yes
    input(type='radio' name='inputRadio' value='false')
    | No
    br
    input(type='button' value='submit' onclick='submitCode()' name=submit)
